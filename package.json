{
  "name": "express-react-ssr-template",
  "version": "0.1.0",
  "description": "Simple template for express app.",
  "main": "src/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "PORT=3000 node src/app.js",
    "dev": "PORT=3000 nodemon src/app.js --ext js,jsx",
    "add-migration": "npx sequelize-mig migration:make -n",
    "db-up": "npx sequelize db:create; node db/models/index.js",
    "db-down": "npx sequelize db:drop"
  },
  "keywords": ["express", "react-ssr", "template"],
  "author": "Hristy",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.9",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sequelize": "^6.25.6"
  },
  "devDependencies": {
    "eslint": "^8.27.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.5.2",
    "sequelize-mig": "^3.1.3"
  }
}
